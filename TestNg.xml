<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Automation">
    <parameter name="broswer" value="chrome"></parameter>

    <listeners>
        <listener class-name ="com.testng.listeners.TestListener"> </listener>
    </listeners>

    <test name ="Login">
        <classes>
            <class name="com.testng.testcases.LoginTest"/>
            <class name="com.testng.testcases.UserMenuTest"/>
            <class name="com.testng.testcases.MyProfileTest"/>
        </classes>
    </test>


</suite>